{
  "name": "docker-ng-cli",
  "version": "0.10.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.18.0",
    "npm": ">=6"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:http": "yarn angular-http-server --path dist --p 9000",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "tslint:check": "npx tslint-config-prettier-check ./tslint.json",
    "slint:base": "stylelint --syntax scss src/**/*.scss",
    "slint:fix": "yarn slint:base --fix",
    "format:base": "prettier src/**/*.ts",
    "format:check": "yarn format:base --list-different",
    "format:fix": "yarn format:base --write",
    "e2e": "ng e2e",
    "deploy": "npx gulp -f gulpfile.ts",
    "migrate:rxjs": "npx tslint -c migrate-rxjs.tslint.json --project src/tsconfig.app.json --fix",
    "docker": "docker build -t ng-cli . && docker run -d -p 4200:80 ng-cli"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "~10.2.5",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.2.0",
    "@angular/material": "~10.2.5",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@angular/service-worker": "~10.2.0",
    "@angular/youtube-player": "~10.2.5",
    "@covalent/core": "^3.1.0",
    "@ngneat/until-destroy": "^8.0.2",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/router-store": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "core-js": "^3.2.1",
    "ngrx-store-freeze": "^0.2.4",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.0",
    "@angular/cli": "10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/language-service": "~10.2.0",
    "@types/jasmine": "^3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.1",
    "angular-http-server": "^1.9.0",
    "codelyzer": "^6.0.1",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulpclass": "^0.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "material-design-icons": "^3.0.1",
    "normalize.css": "^8.0.1",
    "prettier": "~2.1.2",
    "protractor": "~7.0.0",
    "run-sequence": "^2.2.1",
    "stylelint": "13.7.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.18.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typeface-ibm-plex-sans": "^1.1.13",
    "typescript": "~4.0.3"
  }
}
