{
  "name": "docker-ng-cli",
  "version": "0.11.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.18.0",
    "npm": ">=6"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --hmr",
    "serve:http": "yarn angular-http-server --path dist --p 9000",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "tslint:check": "npx tslint-config-prettier-check ./tslint.json",
    "slint:base": "stylelint --syntax scss src/**/*.scss",
    "slint:fix": "yarn slint:base --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:scss": "prettier-stylelint --write \"src/**/*.scss\"",
    "postinstall": "npx patch-package",
    "e2e": "ng e2e",
    "deploy": "npx gulp -f gulpfile.ts",
    "migrate:rxjs": "npx tslint -c migrate-rxjs.tslint.json --project src/tsconfig.app.json --fix",
    "docker": "docker build -t ng-cli . && docker run -d -p 4200:80 ng-cli"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "~11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.1.0",
    "@angular/material": "~11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "@angular/service-worker": "~11.1.0",
    "@angular/youtube-player": "~11.1.0",
    "@covalent/core": "^3.1.0",
    "@ngneat/until-destroy": "^8.0.2",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/router-store": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "core-js": "^3.8.3",
    "ngrx-store-freeze": "^0.2.4",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1101.0",
    "@angular/cli": "11.1.0",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/language-service": "~11.1.0",
    "@fontsource/ibm-plex-sans": "^4.1.0",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.22",
    "angular-http-server": "^1.9.0",
    "codelyzer": "^6.0.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulpclass": "^0.2.0",
    "import-sort-style-module": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "material-design-icons": "^3.0.1",
    "normalize.css": "^8.0.1",
    "patch-package": "^6.2.2",
    "prettier": "~2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~7.0.0",
    "run-sequence": "^2.2.1",
    "stylelint": "13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "3.18.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.1.3"
  },
  "importSort": {
    ".ts": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
